   "source": [
    "def fibb(n):\n",
    " if num < 1:\n",
    "  return 0\n",
    " else:\n",
    "     n1=0\n",
    "     n2=1\n",
    "     count=0\n",
    "     while count<n:\n",
    "         print(n1)\n",
    "         add = n1 + n2\n",
    "         n1 = n2\n",
    "         n2 = add\n",
    "         count+=1\n",
    "\n",
    "num = int(input('enter number of terms: '))\n",
    "fibb(num)"
   ]
  }
